    "@types/xml2js": "^0.4.8",
    "aws-lambda": "^1.0.6",
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "localhost-lambdas": "^0.2.14",
    "roamjs-scripts": "^0.9.12",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.44.2",
    "@blueprintjs/select": "^3.16.2",
    "axios": "^0.21.1",
    "crypto-js": "^4.1.1",
    "otter.ai-api": "^1.0.2",
    "randomstring": "^1.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "roamjs-components": "^0.42.4",
    "xml2js": "^0.4.23"
