{
  "name": "otter",
  "version": "1.0.0",
  "description": "Import your audio notes from Otter directly into Roam!",
  "main": "./build/main.js",
  "scripts": {
    "build": "roamjs-scripts build",
    "dev": "roamjs-scripts dev",
    "lambdas": "cross-env NODE_ENV=production roamjs-scripts lambdas",
